<p id="notice"><%= notice %></p>

<h1>Listing Wearables</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wearables.each do |wearable| %>
      <tr>
        <td><%= wearable.description %></td>
        <td><%= link_to 'Show', wearable %></td>
        <td><%= link_to 'Edit', edit_wearable_path(wearable) %></td>
        <td><%= link_to 'Destroy', wearable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Wearable', new_wearable_path %>
